{
    "name": "pz/doctrine-rest",
    "description": "Rest API library for Doctrine 2 ORM",
    "keywords": ["doctrine", "rest"],
    "type": "library",
    "require": {
        "php": "^7.0",
        "doctrine/orm": "^2.5",
        "league/fractal": "^0.17.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "mockery/mockery": "^1.0",
        "phpunit/php-code-coverage": "^5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "PavelZ",
            "email": "r3volut1oner@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pz\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-0": {
        "Tests\\": "tests/"
      }
    },
    "minimum-stability": "dev"
}
